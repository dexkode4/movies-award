     <favContext.Provider value={contextValue}>\n            {children}\n        </favContext.Provider>\n    )\n\n};\n\nexport default FavContextProvider;\n","/Users/macbook/Desktop/Dexkode/ReactProject/movies-award/src/Components/MovieCard/MovieCard.components.tsx",[],"/Users/macbook/Desktop/Dexkode/ReactProject/movies-award/src/Components/FavoriteCount/index.tsx",[],["69","70"],"/Users/macbook/Desktop/Dexkode/ReactProject/movies-award/src/pages/favlist/favlist.tsx",[],"/Users/macbook/Desktop/Dexkode/ReactProject/movies-award/src/pages/favlist/favlist.component.tsx",[],{"ruleId":"71","severity":1,"message":"72","line":1,"column":29,"nodeType":"73","messageId":"74","endLine":1,"endColumn":38},{"ruleId":"71","severity":1,"message":"75","line":10,"column":9,"nodeType":"73","messageId":"74","endLine":10,"endColumn":14},{"ruleId":"76","replacedBy":"77"},{"ruleId":"78","replacedBy":"79"},{"ruleId":"71","severity":1,"message":"80","line":6,"column":10,"nodeType":"73","messageId":"74","endLine":6,"endColumn":24},{"ruleId":"76","replacedBy":"81"},{"ruleId":"78","replacedBy":"82"},{"ruleId":"76","replacedBy":"83"},{"ruleId":"78","replacedBy":"84"},{"ruleId":"85","severity":1,"message":"86","line":20,"column":9,"nodeType":"87","endLine":20,"endColumn":16,"suggestions":"88"},{"ruleId":"71","severity":1,"message":"89","line":30,"column":11,"nodeType":"73","messageId":"74","endLine":30,"endColumn":30},{"ruleId":"90","severity":1,"message":"91","line":36,"column":29,"nodeType":"92","messageId":"93","endLine":36,"endColumn":30,"fix":"94"},{"ruleId":"71","severity":1,"message":"95","line":1,"column":32,"nodeType":"73","messageId":"74","endLine":1,"endColumn":42},{"ruleId":"85","severity":1,"message":"96","line":59,"column":9,"nodeType":"87","endLine":59,"endColumn":20,"suggestions":"97"},{"ruleId":"76","replacedBy":"98"},{"ruleId":"78","replacedBy":"99"},"@typescript-eslint/no-unused-vars","'useEffect' is defined but never used.","Identifier","unusedVar","'state' is assigned a valu